              },
              "style": {
                "classes": "Button/Standard",
                "marginBottom": "6px",
                "marginRight": "6px"
              },
              "text": "No"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "FlexContainer_0"
        },
        "position": {
          "basis": "38px",
          "shrink": 0
        },
        "props": {
          "justify": "center",
          "style": {
            "marginBottom": "10px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}