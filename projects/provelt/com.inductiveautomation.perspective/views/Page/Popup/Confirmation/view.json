        "position": {
          "grow": 1
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tparams \u003d dashboard.copyDictionary(self.view.params.function.params)\n\t\t\n\tif len(params):\n\t\tscript \u003d \"%s(self, params)\" % self.view.params.function.script\n\telse:\n\t\tscript \u003d \"%s(self)\" % self.view.params.function.script\n\t\n\ttry:\n\t\texec(script)\n\texcept:\n\t\tpass\n\t\t\n\tsystem.perspective.closePopup(id\u003d\"confirmation\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Save"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/check"
                }
              },
              "style": {
                "classes": "Button/Standard",
                "marginBottom": "6px",
                "marginRight": "6px"
              },
              "text": "Yes"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "draggable": false,
                    "id": "confirmation",
                    "modal": false,
                    "overlayDismiss": false,
                    "resizable": false,
                    "showCloseIcon": false,
                    "type": "close",
                    "viewPath": ""
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Save_0"
            },
            "position": {
              "basis": "100px"
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/cancel"
