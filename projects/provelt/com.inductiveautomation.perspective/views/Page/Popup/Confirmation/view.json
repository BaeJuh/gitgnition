{
  "custom": {},
  "params": {
    "class": "Text",
    "display": "Can you do this?",
    "function": {
      "params": {},
      "script": ""
    },
    "icon": "help"
  },
  "propConfig": {
    "params.class": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.display": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.function": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.function.params": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.function.script": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 400
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "48px",
              "shrink": 0
            },
            "propConfig": {
              "props.path": {
                "binding": {
                  "config": {
                    "expression": "\"material/\" + {view.params.icon}"
                  },
                  "type": "expr"
                }
              },
              "props.style.classes": {
                "binding": {
                  "config": {
                    "expression": "\"Popup/\" + {view.params.class}"
                  },
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "marginLeft": "10px"
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "props.style.classes": {
                "binding": {
                  "config": {
                    "expression": "\"Popup/\" + {view.params.class}"
                  },
                  "type": "expr"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.params.display"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "alignVertical": "center",
              "style": {
