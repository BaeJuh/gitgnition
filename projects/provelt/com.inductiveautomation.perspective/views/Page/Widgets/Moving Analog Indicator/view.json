{
  "custom": {
    "pollingRate": 0,
    "tagPaths": [
      {
        "alias": "tag"
      }
    ]
  },
  "params": {
    "defaultRangeColor": "#EEEEEE",
    "desiredHigh": 65,
    "desiredLow": 40,
    "desiredRangeColor": "#B8DAFF",
    "highAlarm": 90,
    "highHighAlarm": null,
    "highInterlock": null,
    "inactiveAlarmColor": "#888888",
    "indicatorColor": "#888888",
    "interlockColor": "#000000",
    "level1AlarmColor": "#FF0000",
    "level2AlarmColor": "#FFFF00",
    "lowAlarm": 10,
    "lowInterlock": null,
    "lowLowAlarm": null,
    "maxValue": 100,
    "minValue": 0,
    "polling": false,
    "pollingRate": 1,
    "rangeMinutes": 5,
    "reverseIndicator": false,
    "setpointColor": "#888888",
    "setpointValue": null,
    "showSparkline": true,
    "showValue": false,
    "sparklineColor": "#888888",
    "sparklineWidth": 0.5,
    "tagPath": ""
  },
  "propConfig": {
    "custom.pollingRate": {
      "binding": {
        "config": {
          "expression": "if(toBoolean({view.params.polling}), {view.params.pollingRate}, 0)"
        },
        "type": "expr"
      },
      "persistent": true
    },
    "custom.tagPaths": {
      "persistent": true
    },
    "custom.tagPaths[0].path": {
      "binding": {
        "config": {
          "path": "view.params.tagPath"
        },
        "type": "property"
      }
    },
    "params.defaultRangeColor": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.desiredHigh": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.desiredLow": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.desiredRangeColor": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.highAlarm": {
