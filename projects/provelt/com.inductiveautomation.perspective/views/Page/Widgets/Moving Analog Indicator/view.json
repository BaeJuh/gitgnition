      "persistent": true
    },
    "params.highHighAlarm": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.highInterlock": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.inactiveAlarmColor": {
      "paramDirection": "input",
      "persistent": true
