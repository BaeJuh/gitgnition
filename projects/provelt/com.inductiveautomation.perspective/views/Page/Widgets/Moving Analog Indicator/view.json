      "persistent": true
    },
    "params.lowAlarm": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.lowInterlock": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.lowLowAlarm": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.maxValue": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.minValue": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.polling": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.pollingRate": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.rangeMinutes": {
