{
  "custom": {},
  "params": {
    "icon": "home",
    "iconColor": "#888888",
    "numberFormat": "#,##0.00",
    "showIcon": true,
    "tagPath": "",
    "units": "%",
    "valueColor": "#000080"
  },
  "propConfig": {
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.iconColor": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.numberFormat": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.showIcon": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.units": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.valueColor": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 25,
      "width": 228
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "24px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "toBoolean({view.params.showIcon})"
              },
              "type": "expr"
            }
          },
          "props.color": {
            "binding": {
              "config": {
                "path": "view.params.iconColor"
              },
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "expression": "\"material/\" + {view.params.icon}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "marginRight": "6px"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.style.color": {
            "binding": {
              "config": {
                "path": "view.params.valueColor"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "1": "{view.params.tagPath}"
                },
                "tagPath": "{1}"
              },
              "transforms": [
                {
                  "expression": "try(numberFormat({value}, {view.params.numberFormat}), \"\")",
                  "type": "expression"
                }
              ],
              "type": "tag"
            }
          }
        },
        "props": {
          "style": {
            "fontWeight": "bold",
            "marginRight": "4px"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_1"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.units"
              },
              "type": "property"
            }
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "justify": "center"
    },
    "type": "ia.container.flex"
  }
}