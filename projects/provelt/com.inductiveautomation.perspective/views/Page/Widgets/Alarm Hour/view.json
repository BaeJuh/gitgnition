    "propConfig": {
      "custom.alarmByHour": {
        "binding": {
          "config": {
            "parameters": {
              "endDate": "{this.custom.endDate}",
              "startDate": "{this.custom.startDate}"
            },
            "queryPath": "Alarms/Alarms By Hour",
            "returnFormat": "json"
          },
          "type": "query"
        }
      },
      "custom.endDate": {
        "binding": {
          "config": {
            "expression": "now({view.params.pollRate}*1000)"
          },
          "type": "expr"
        }
      },
      "custom.startDate": {
        "binding": {
          "config": {
            "expression": "dateArithmetic({this.custom.endDate}, -1 * {view.params.interval}, \"minute\")"
          },
          "type": "expr"
        }
