{
  "custom": {},
  "params": {},
  "propConfig": {
    "params.feature": {
      "paramDirection": "input"
    },
    "params.tier1": {
      "paramDirection": "input"
    },
    "params.tier2": {
      "paramDirection": "input"
    }
  },
  "props": {
    "defaultSize": {
      "width": 860
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Title"
        },
        "position": {
          "basis": "36px",
          "shrink": 0
        },
        "propConfig": {
          "props.params.name": {
            "binding": {
              "config": {
                "expression": "if({session.custom.dashboard.objects.edit.action.action} \u003d \"add\", \"Add\", \"Edit\") + \" Dashboard\""
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "icon": "settings"
          },
          "path": "Page/Embedded/Title"
        },
        "type": "ia.display.view"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Details"
            },
            "props": {
              "path": "Page/Configuration/Details",
              "style": {
                "padding": "10px"
              }
            },
            "type": "ia.display.view"
          },
          {
            "meta": {
              "name": "Widgets"
            },
            "position": {
              "tabIndex": 1
            },
            "props": {
              "path": "Page/Configuration/Widgets",
              "style": {
                "padding": "10px"
              }
            },
            "type": "ia.display.view"
          }
        ],
        "meta": {
          "name": "Tabs"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.currentTabIndex": {
            "binding": {
              "config": {
                "expression": "if({session.custom.dashboard.objects.edit.action.action} \u003d \"edit\", 1, 0)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "contentStyle": {
            "borderStyle": "none"
          },
          "menuStyle": {
            "backgroundColor": "var(--neutral-30)"
          },
          "menuType": "modern",
          "tabStyle": {
            "active": {
              "classes": "Menu/Tab_Selected"
            },
            "inactive": {
              "classes": "Menu/Tab"
            }
          },
          "tabs": [
            "Details",
            "Widgets"
          ]
        },
        "type": "ia.container.tab"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tdashboard.syncDashboardDB(self)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Save"
            },
            "position": {
              "basis": "100px",
              "shrink": 0
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/save"
                }
              },
              "style": {
                "classes": "Button/Standard",
                "marginBottom": "6px",
                "marginRight": "6px"
              },
              "text": "Save"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tdashboard.showConfirmation(\"Are you sure you want to delete this dashboard?\", \"dashboard.deleteDashboard\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Delete"
            },
            "position": {
              "basis": "100px",
              "shrink": 0
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/delete"
                }
              },
              "style": {
                "classes": "Button/Error",
                "marginBottom": "6px"
              },
              "text": "Delete"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "Actions"
        },
        "position": {
          "basis": "38px",
          "shrink": 0
        },
        "props": {
          "justify": "center",
          "style": {
            "marginBottom": "10px"
          },
          "wrap": "wrap"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "borderLeftColor": "var(--neutral-40)",
        "borderLeftStyle": "solid",
        "borderLeftWidth": "1px",
        "classes": "Page"
      }
    },
    "type": "ia.container.flex"
  }
}