{
  "custom": {},
  "events": null,
  "params": {
    "configuration": {},
    "value": ""
  },
  "permissions": null,
  "propConfig": {
    "params.configuration": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 297
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Field"
        },
        "position": {
          "basis": "40px",
          "display": true,
          "grow": 0,
          "shrink": 0
        },
        "propConfig": {
          "props.allowCustomOptions": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"allowCustomOptions\", False)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"options\", [])",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.placeholder.text": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"placeholder\", \"Select...\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.showClearIcon": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"showClearIcon\", False)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.value"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "placeholder": {},
          "style": {
            "classes": "Form/Field"
          }
        },
        "type": "ia.input.dropdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}