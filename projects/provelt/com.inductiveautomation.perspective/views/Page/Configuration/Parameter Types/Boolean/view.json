{
  "custom": {},
  "events": null,
  "params": {
    "configuration": {},
    "value": false
  },
  "permissions": null,
  "propConfig": {
    "params.configuration": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 32,
      "width": 297
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Field"
        },
        "position": {
          "basis": "auto",
          "display": true,
          "grow": 0,
          "shrink": 0
        },
        "propConfig": {
          "props.color.selected": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"selectedColor\", \"#2196f3\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.color.unselected": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"selectedColor\", \"#FFFFFF\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.label.text": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"text\", \"\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.selected": {
            "binding": {
              "config": {
                "expression": "toBoolean({view.params.value})"
              },
              "type": "expr"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif origin \u003d\u003d \"Browser\":\n\t\tself.view.params.value \u003d currentValue.value"
            }
          }
        },
        "props": {
          "color": {},
          "label": {}
        },
        "type": "ia.input.toggle-switch"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.flex"
  }
}