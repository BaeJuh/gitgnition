{
  "custom": {},
  "events": null,
  "params": {
    "configuration": {},
    "value": ""
  },
  "permissions": null,
  "propConfig": {
    "params.configuration": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.value": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 401,
      "width": 297
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Field"
        },
        "position": {
          "basis": "150px",
          "display": true,
          "grow": 0,
          "shrink": 0
        },
        "propConfig": {
          "props.deferUpdates": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"deferUpdates\", True)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.placeholder": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"placeholder\", \"\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.rejectUpdatesWhileFocused": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"rejectUpdatesWhileFocused\", True)",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.resize": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"resize\", \"both\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.value"
              },
              "transforms": [
                {
                  "code": "\timport json\n\ttry:\n\t\treturn json.dumps(json.loads(value), indent\u003d4)\n\texcept:\n\t\treturn \"{}\" if value \u003d\u003d None or len(value) \u003d\u003d 0 else value",
                  "type": "script"
                }
              ],
              "type": "property"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif origin \u003d\u003d \"Browser\":\n\t\tself.view.params.value \u003d currentValue.value"
            }
          },
          "props.wrap": {
            "binding": {
              "config": {
                "path": "view.params.configuration"
              },
              "transforms": [
                {
                  "code": "\treturn dashboard.getConfiguration(value, \"wrap\", \"soft\")",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "Form/Field"
          }
        },
        "type": "ia.input.text-area"
      },
      {
        "meta": {
          "name": "Valid"
        },
        "position": {
          "basis": "auto",
          "display": true,
          "grow": 0,
          "shrink": 1
        },
        "propConfig": {
          "props.selected": {
            "binding": {
              "config": {
                "path": "../Field.props.text"
              },
              "transforms": [
                {
                  "code": "\timport json\n\t\n\ttry:\n\t\tjson.dumps(json.loads(value))\n\t\treturn True\n\texcept:\n\t\treturn False",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.style.backgroundColor": {
            "binding": {
              "config": {
                "bidirectional": false,
                "path": "this.props.selected"
              },
              "transforms": [
                {
                  "code": "\tif value:\n\t\treturn \"#8AFF8A\"\n\telse:\n\t\treturn \"#FFCA8A\"",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "bidirectional": false,
                "path": "this.props.selected"
              },
              "transforms": [
                {
                  "code": "\tif value:\n\t\treturn \"Valid\"\n\telse:\n\t\treturn \"Invalid\"",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "enabled": false,
          "style": {
            "textAlign": "center"
          }
        },
        "type": "ia.input.checkbox"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}