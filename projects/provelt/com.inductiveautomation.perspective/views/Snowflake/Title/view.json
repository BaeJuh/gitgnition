{
  "custom": {},
  "params": {
    "icon": "home",
    "iconColor": "var(--neutral-80)",
    "messageAction": {
      "icon": "refresh",
      "messageType": "",
      "payload": {}
    },
    "text": "Home"
  },
  "propConfig": {
    "params.icon": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.iconColor": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.messageAction": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.text": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 56,
      "width": 600
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "propConfig": {
          "props.color": {
            "binding": {
              "config": {
                "path": "view.params.iconColor"
              },
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "expression": "\"material/\" + {view.params.icon}"
              },
              "type": "expr"
            },
            "persistent": false
          }
        },
        "props": {
          "style": {
            "classes": "Snowflake/Icon",
            "marginLeft": "10px"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "hasDelegate": true,
          "name": "Label"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.text"
              },
              "type": "property"
            },
            "persistent": false
          }
        },
        "props": {
          "style": {
            "classes": "Snowflake/Title/Text"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\tmessageType \u003d self.view.params.messageAction.messageType\n\tpayload \u003d self.view.params.messageAction.payload\n\tsystem.perspective.sendMessage(messageType\u003dmessageType, payload\u003dpayload)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Icon_0"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.params.messageAction.messageType} !\u003d \"\""
              },
              "type": "expr"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "expression": "\"material/\" + {view.params.messageAction.icon}"
              },
              "type": "expr"
            },
            "persistent": false
          }
        },
        "props": {
          "style": {
            "classes": "Snowflake/Icon",
            "cursor": "pointer",
            "marginLeft": "10px",
            "marginRight": "10px"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "classes": "Snowflake/Title/Title",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}