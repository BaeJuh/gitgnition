            "stageDB": "{session.custom.stageDB}"
          },
          "queryPath": "Snowflake/Instance Birth",
          "returnFormat": "json"
        },
        "transforms": [
          {
            "code": "\tif value !\u003d None and len(value):\n\t\treturn system.util.jsonDecode(value[0][\"TEMPLATE_DEFINITION\"])\n\treturn None",
            "type": "script"
          }
        ],
        "type": "query"
      },
      "persistent": true
    },
    "custom.color": {
      "binding": {
        "config": {
          "path": "view.custom.birthMessage"
        },
        "transforms": [
          {
            "code": "\tif value !\u003d None and \"properties\" in value:\n\t\tif \"typeColor\" in value.properties:\n\t\t\tfrom java.lang import Integer\n\t\t\treturn \"#%s\" % Integer.toHexString(value.properties.typeColor.value)[2:]\n\t\n\treturn \"var(--neutral-80)\"",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
    "custom.templateDef": {
      "binding": {
        "config": {
          "path": "view.params.templateDef"
        },
        "transforms": [
          {
            "code": "\ttry:\n\t\treturn system.util.jsonDecode(value)\n\texcept:\n\t\treturn {}",
            "type": "script"
          }
        ],
        "type": "property"
      },
      "persistent": true
    },
