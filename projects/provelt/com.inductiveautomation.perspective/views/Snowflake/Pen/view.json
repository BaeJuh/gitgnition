{
  "custom": {},
  "params": {
    "color": "#FFFFFF",
    "enabled": true,
    "name": "Supply fan speed",
    "selection": "multiple"
  },
  "propConfig": {
    "params.color": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.enabled": {
      "paramDirection": "inout",
      "persistent": true
    },
    "params.name": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selection": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 22,
      "width": 212
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Color"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "propConfig": {
          "props.fill": {
            "binding": {
              "config": {
                "path": "view.params.color"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "elements": [
            {
              "height": "60%",
              "rx": "3px",
              "type": "rect",
              "width": "60%",
              "x": "20%",
              "y": "20%"
            }
          ],
          "viewBox": "0 0 24 24"
        },
        "type": "ia.shapes.svg"
      },
      {
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "basis": "20px",
          "shrink": 0
        },
        "propConfig": {
          "props.selected": {
            "binding": {
              "config": {
                "path": "view.params.enabled"
              },
              "type": "property"
            },
            "onChange": {
              "enabled": null,
              "script": "\tif origin \u003d\u003d \"Browser\" and currentValue.value:\n\t\tsystem.perspective.sendMessage(\"metricSelection\", {\"name\":self.view.params.name})\n\tself.view.params.enabled \u003d currentValue.value"
            }
          }
        },
        "props": {
          "checkedIcon": {
            "color": {
              "enabled": "var(--neutral-100)"
            },
            "style": {
              "maxHeight": "17px"
            }
          },
          "text": "",
          "uncheckedIcon": {
            "style": {
              "maxHeight": "17px"
            }
          }
        },
        "scripts": {
          "customMethods": [],
          "extensionFunctions": null,
          "messageHandlers": [
            {
              "messageType": "metricSelection",
              "pageScope": true,
              "script": "\tif payload[\"name\"] !\u003d self.view.params.name and self.view.params.selection \u003d\u003d \"single\":\n\t\tself.props.selected \u003d False",
              "sessionScope": false,
              "viewScope": false
            }
          ]
        },
        "type": "ia.input.checkbox"
      },
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.name"
              },
              "transforms": [
                {
                  "code": "\tif \u0027AHU\u0027 in value:\n\t\treturn value[5:len(value)]\n\telse:\n\t\treturn value",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "color": "--neutral-90",
            "fontSize": "11px",
            "overflow": "hidden",
            "paddingLeft": "5px"
          }
        },
        "type": "ia.display.label"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.flex"
  }
}