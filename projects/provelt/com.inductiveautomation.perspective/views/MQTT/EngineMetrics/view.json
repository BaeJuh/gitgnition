            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "expression": "now(30000)"
                  },
                  "transforms": [
                    {
                      "code": "\ttry:\n\t\tfrom com.inductiveautomation.ignition.gateway import IgnitionGateway\n\t\tcontext \u003d IgnitionGateway.get()\n\t\ttagManager \u003d context.getTagManager()\n\t\ttagProvider \u003d tagManager.getTagProvider(\"MQTT Engine\")\n\t\ttagCount \u003d tagProvider.getStatusInformation().get().getTagCount()\n\t\treturn tagCount\n\texcept:\n\t\treturn 0",
                      "type": "script"
                    },
                    {
                      "expression": "if({value} \u003d null, 0, numberFormat({value}, \"#,##0\"))",
                      "type": "expression"
                    }
                  ],
                  "type": "expr"
                }
              }
            },
            "props": {
              "style": {
                "classes": "MQTT/Value"
              }
            },
            "type": "ia.display.label"
          }
        ],
