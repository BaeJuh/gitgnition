                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "server": "{view.params.server}"
                    },
                    "tagPath": "[MQTT Engine]Engine Info/MQTT Clients/Default Set/{server}/Online"
                  },
                  "transforms": [
                    {
                      "fallback": "#808080",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": false,
                          "output": "#D90000"
                        },
                        {
                          "input": true,
                          "output": "#00AC00"
                        }
                      ],
                      "outputType": "color",
                      "type": "map"
                    }
                  ],
                  "type": "tag"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "server": "{view.params.server}"
                    },
                    "tagPath": "[MQTT Engine]Engine Info/MQTT Clients/Default Set/{server}/Online"
                  },
                  "transforms": [
                    {
                      "fallback": "No",
                      "inputType": "scalar",
                      "mappings": [
                        {
                          "input": false,
                          "output": "No"
                        },
                        {
                          "input": true,
                          "output": "Yes"
                        }
                      ],
                      "outputType": "scalar",
                      "type": "map"
                    }
                  ],
                  "type": "tag"
                }
              }
            },
            "props": {
              "style": {
                "borderRadius": "4px",
                "classes": "MQTT/Value",
                "color": "#FFFFFF",
                "textAlign": "center"
              }
            },
            "type": "ia.display.label"
