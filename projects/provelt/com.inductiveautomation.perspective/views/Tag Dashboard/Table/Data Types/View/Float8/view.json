{
  "custom": {
    "numberFormat": "#,##0.##",
    "readOnly": false,
    "value": 46
  },
  "params": {
    "tagPath": "[default]Writeable/WriteableInteger1"
  },
  "propConfig": {
    "custom.numberFormat": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "tagPath": "{view.params.tagPath}"
          },
          "tagPath": "{tagPath}.FormatString"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.readOnly": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "tagPath": "{view.params.tagPath}"
          },
          "tagPath": "{tagPath}.ReadOnly"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "custom.value": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "tagPath": "{view.params.tagPath}"
          },
          "tagPath": "{tagPath}"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 32,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "expression": "numberFormat({view.custom.value}, {view.custom.numberFormat})"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "classes": "TagDashboard/Value"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_1"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "tagPath": "{view.params.tagPath}"
                },
                "tagPath": "{tagPath}.EngUnit"
              },
              "type": "tag"
            }
          }
        },
        "props": {
          "style": {
            "classes": "TagDashboard/Value",
            "marginLeft": "5px"
          }
        },
        "type": "ia.display.label"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\ttagPath \u003d self.view.params.tagPath\n\tTagDashboard.editTag(tagPath)"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Icon"
        },
        "position": {
          "basis": "24px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{session.custom.settings.tagEdit} \u0026\u0026 !{view.custom.readOnly}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "path": "material/edit",
          "style": {
            "cursor": "pointer",
            "marginLeft": "5px"
          }
        },
        "type": "ia.display.icon"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "style": {
        "overflow": "hidden"
      }
    },
    "type": "ia.container.flex"
  }
}