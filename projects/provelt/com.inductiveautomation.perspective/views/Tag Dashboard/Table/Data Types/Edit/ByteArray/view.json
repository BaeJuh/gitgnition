{
  "custom": {},
  "params": {
    "tagPath": "[default]Writeable/ArrayTag2"
  },
  "propConfig": {
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 200,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label_0"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "tagPath": "{view.params.tagPath}"
                    },
                    "tagPath": "{tagPath}"
                  },
                  "transforms": [
                    {
                      "code": "\treturn \"None\" if value \u003d\u003d None else \"Array[%d]\" % len(value)",
                      "type": "script"
                    }
                  ],
                  "type": "tag"
                }
              }
            },
            "props": {
              "style": {
                "classes": "TagDashboard/Value"
              }
            },
            "type": "ia.display.label"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\ttagPath \u003d self.view.params.tagPath\n\ttagName \u003d system.tag.readBlocking([tagPath+\".Name\"])[0].value\n\tbytes \u003d system.tag.readBlocking([tagPath])[0].value\n\tsystem.perspective.download(tagName, bytes, \"application/octet-stream\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "24px",
              "shrink": 0
            },
            "props": {
              "path": "material/cloud_download",
              "style": {
                "classes": "TagDashboard/Title/Icon",
                "cursor": "pointer"
              }
            },
            "type": "ia.display.icon"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "style": {
            "marginBottom": "10px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "events": {
          "component": {
            "onFileReceived": {
              "config": {
                "script": "\ttagPath \u003d self.view.params.tagPath\n\tbytes \u003d event.file.getBytes()\n\tsystem.tag.writeBlocking([tagPath], [bytes])"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "FileUpload"
        },
        "position": {
          "basis": "400px",
          "grow": 1
        },
        "props": {
          "fileSizeLimit": 2,
          "maxUploads": 1
        },
        "type": "ia.input.fileupload"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}