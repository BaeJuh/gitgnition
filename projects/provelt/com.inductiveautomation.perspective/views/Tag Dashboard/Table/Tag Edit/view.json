{
  "custom": {
    "dataType": "String"
  },
  "params": {
    "tagPath": "[default]Writeable/WriteableString1"
  },
  "propConfig": {
    "custom.dataType": {
      "binding": {
        "config": {
          "fallbackDelay": 2.5,
          "mode": "indirect",
          "references": {
            "tagPath": "{view.params.tagPath}"
          },
          "tagPath": "{tagPath}.DataType"
        },
        "type": "tag"
      },
      "persistent": true
    },
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 400,
      "width": 500
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Label"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "text": "Tag:"
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_0"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.tagPath"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "TagDashboard/Value"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Label_1"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "style": {
            "marginTop": "10px"
          },
          "text": "Value:"
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "EmbeddedView"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.params.tagPath": {
            "binding": {
              "config": {
                "path": "view.params.tagPath"
              },
              "type": "property"
            }
          },
          "props.path": {
            "binding": {
              "config": {
                "expression": "\"Tag Dashboard/Table/Data Types/Edit/\" + {view.custom.dataType}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "style": {
            "marginBottom": "10px"
          },
          "useDefaultViewHeight": true
        },
        "type": "ia.display.view"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "draggable": true,
                    "id": "tag-edit",
                    "modal": false,
                    "overlayDismiss": false,
                    "resizable": true,
                    "showCloseIcon": true,
                    "type": "close",
                    "viewPath": "",
                    "viewportBound": false
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Button"
            },
            "position": {
              "basis": "70px",
              "shrink": 0
            },
            "props": {
              "text": "Close"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "basis": "32px",
          "shrink": 0
        },
        "props": {
          "justify": "center"
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "padding": "10px"
      }
    },
    "type": "ia.container.flex"
  }
}