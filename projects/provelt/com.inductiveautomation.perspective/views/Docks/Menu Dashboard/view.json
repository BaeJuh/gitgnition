{
  "custom": {
    "dbValid": true,
    "key": "value"
  },
  "params": {},
  "propConfig": {
    "custom.dbValid": {
      "binding": {
        "config": {
          "path": "session.custom.dbValid"
        },
        "type": "property"
      },
      "persistent": true
    },
    "custom.key": {
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "width": 260
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Status"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "path": "Page/Configuration/Status",
          "style": {
            "color": "var(--neutral-100)",
            "fontSize": "14px",
            "paddingLeft": "10px",
            "paddingRight": "10px"
          }
        },
        "type": "ia.display.view"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Features"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "this.props.items"
                  },
                  "transforms": [
                    {
                      "code": "\treturn len(value) \u003e 0",
                      "type": "script"
                    }
                  ],
                  "type": "property"
                }
              },
              "props.items": {
                "binding": {
                  "config": {
                    "struct": {
                      "dashboards": "{session.custom.dashboard.dashboards.dashboards}",
                      "path": "{page.props.path}"
                    },
                    "waitOnAll": true
                  },
                  "transforms": [
                    {
                      "code": "\treturn dashboard.getMenu(value)",
                      "type": "script"
                    }
                  ],
                  "type": "expr-struct"
                }
              }
            },
            "props": {
              "backActionStyle": {
                "classes": "Menu/All_Header"
              },
              "backActionText": "Back",
              "headerStyle": {
                "classes": "Menu/Item_Selected"
              },
              "itemStyle": {
                "classes": "Menu/Item"
              },
              "style": {
                "classes": "Menu/Menu"
              }
            },
            "scripts": {
              "customMethods": [],
              "extensionFunctions": null,
              "messageHandlers": [
                {
                  "messageType": "dashboardRefresh",
                  "pageScope": true,
                  "script": "\tself.refreshBinding(\"props.items\")",
                  "sessionScope": false,
                  "viewScope": false
                }
              ]
            },
            "type": "ia.navigation.menutree"
          }
        ],
        "meta": {
          "name": "Dashboard Tree"
        },
        "position": {
          "grow": 1
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "children": [
              {
                "meta": {
                  "name": "Icon"
                },
                "position": {
                  "basis": "32px",
                  "shrink": 0
                },
                "props": {
                  "path": "material/add_circle",
                  "style": {
                    "classes": "Menu/Icon",
                    "cursor": "pointer"
                  }
                },
                "type": "ia.display.icon"
              },
              {
                "meta": {
                  "name": "Label"
                },
                "position": {
                  "basis": "20px",
                  "shrink": 0
                },
                "props": {
                  "style": {
                    "classes": "Menu/Small_Text"
                  },
                  "text": "Add Dashboard"
                },
                "type": "ia.display.label"
              }
            ],
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tdashboard.editDashboard(self)"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Add Dashboard"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "direction": "column",
              "style": {
                "cursor": "pointer",
                "marginRight": "10px",
                "overflow": "hidden"
              }
            },
            "type": "ia.container.flex"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Icon"
                },
                "position": {
                  "basis": "32px",
                  "shrink": 0
                },
                "props": {
                  "path": "material/reorder",
                  "style": {
                    "classes": "Menu/Icon",
                    "cursor": "pointer"
                  }
                },
                "type": "ia.display.icon"
              },
              {
                "meta": {
                  "name": "Label"
                },
                "position": {
                  "basis": "20px",
                  "shrink": 0
                },
                "props": {
                  "style": {
                    "classes": "Menu/Small_Text"
                  },
                  "text": "Reorder"
                },
                "type": "ia.display.label"
              }
            ],
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "draggable": false,
                    "id": "dashboard-tree",
                    "modal": true,
                    "overlayDismiss": true,
                    "resizable": true,
                    "showCloseIcon": true,
                    "title": "Reorder Dashboards",
                    "type": "open",
                    "viewParams": {
                      "id": "-1",
                      "mode": "move"
                    },
                    "viewPath": "Page/Configuration/Dashboard Tree"
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Reorder Dashboard"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "direction": "column",
              "style": {
                "cursor": "pointer",
                "marginRight": "10px",
                "overflow": "hidden"
              }
            },
            "type": "ia.container.flex"
          },
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "type": "ia.display.label"
          },
          {
            "children": [
              {
                "meta": {
                  "name": "Icon"
                },
                "position": {
                  "basis": "32px",
                  "shrink": 0
                },
                "props": {
                  "path": "material/person",
                  "style": {
                    "classes": "Menu/Icon",
                    "cursor": "pointer"
                  }
                },
                "type": "ia.display.icon"
              },
              {
                "meta": {
                  "name": "Label"
                },
                "position": {
                  "shrink": 0
                },
                "propConfig": {
                  "props.text": {
                    "binding": {
                      "config": {
                        "path": "session.props.auth.user.userName"
                      },
                      "transforms": [
                        {
                          "expression": "coalesce({value}, \u0027Guest\u0027)",
                          "type": "expression"
                        }
                      ],
                      "type": "property"
                    }
                  }
                },
                "props": {
                  "style": {
                    "classes": "Menu/Small_Text"
                  }
                },
                "type": "ia.display.label"
              }
            ],
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tif self.session.props.auth.authenticated:\n\t\tsystem.perspective.logout()\n\telse:\n\t\tsystem.perspective.login()"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Signed In"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "direction": "column",
              "style": {
                "cursor": "pointer",
                "marginRight": "10px",
                "overflow": "hidden"
              }
            },
            "type": "ia.container.flex"
          }
        ],
        "meta": {
          "name": "Actions"
        },
        "position": {
          "basis": "50px",
          "shrink": 0
        },
        "props": {
          "style": {
            "margin": "10px"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "Menu/Menu"
      }
    },
    "type": "ia.container.flex"
  }
}