  "params": {
    "color": "#FFFFFF",
    "includeOriginal": false
  },
  "propConfig": {
    "custom.alpha": {
      "binding": {
        "config": {
          "expression": "try(\r\n\tfromhex(substring({view.params.color}, 7, 9)),\r\n\t255\r\n)"
        },
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif origin in [\"Browser\", \"BindingWriteback\"] and currentValue.value is not None:\n\t\tc \u003d self.view.params.color\n\t\tif currentValue.value \u003c 255:\n\t\t\tself.view.params.color \u003d \"{}{:02X}\".format(c[:7], currentValue.value)\n\t\telse:\n\t\t\tself.view.params.color \u003d self.view.params.color[:7]"
      },
      "persistent": true
    },
    "custom.blue": {
      "binding": {
        "config": {
          "expression": "fromhex(substring({view.params.color}, 5, 7))"
        },
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif origin in [\"Browser\", \"BindingWriteback\"] and currentValue.value is not None:\n\t\tc \u003d self.view.params.color\n\t\tself.view.params.color \u003d \"{}{:02X}{}\".format(c[:5], currentValue.value, c[7:9])\n\t\t"
      },
      "persistent": true
    },
    "custom.green": {
      "binding": {
        "config": {
          "expression": "fromhex(substring({view.params.color}, 3, 5))"
        },
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif origin in [\"Browser\", \"BindingWriteback\"] and currentValue.value is not None:\n\t\tc \u003d self.view.params.color\n\t\tself.view.params.color \u003d \"{}{:02X}{}\".format(c[:3], currentValue.value, c[5:9])\n\t\t"
      },
      "persistent": true
    },
    "custom.original": {
      "persistent": true
    },
    "custom.red": {
      "binding": {
        "config": {
          "expression": "fromhex(substring({view.params.color}, 1, 3))"
        },
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif origin in [\"Browser\", \"BindingWriteback\"] and currentValue.value is not None:\n\t\tc \u003d self.view.params.color\n\t\tself.view.params.color \u003d \"#{:02X}{}\".format(currentValue.value, c[3:9])\n\t\t"
      },
      "persistent": true
    },
    "params.color": {
      "binding": {
        "config": {
          "expression": "\"#\" + tohex({view.custom.red}) + tohex({view.custom.green}) + tohex({view.custom.blue})"
        },
        "enabled": false,
        "type": "expr"
      },
      "onChange": {
        "enabled": null,
        "script": "\tif self.custom.original is None:\n\t\tself.custom.original \u003d currentValue.value"
      },
      "paramDirection": "inout",
      "persistent": true
    },
    "params.includeOriginal": {
      "paramDirection": "input",
      "persistent": true
    }
  },
