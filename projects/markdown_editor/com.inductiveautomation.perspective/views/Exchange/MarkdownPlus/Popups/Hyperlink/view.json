{
  "custom": {
    "text": "",
    "url": ""
  },
  "params": {
    "domID": ""
  },
  "propConfig": {
    "custom.text": {
      "access": "PRIVATE",
      "persistent": true
    },
    "custom.url": {
      "access": "PRIVATE",
      "persistent": true
    },
    "params.domID": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 180,
      "width": 450
    }
  },
  "root": {
    "children": [
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "text": "Link Text"
            },
            "type": "ia.display.label"
          },
          {
            "events": {
              "system": {
                "onStartup": {
                  "config": {
                    "script": "\tself.focus()"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "LinkText"
            },
            "position": {
              "basis": "32px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.custom.text"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "placeholder": "(Optional)"
            },
            "type": "ia.input.text-field"
          }
        ],
        "meta": {
          "name": "Row 1"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "direction": "column",
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "grow": 1
            },
            "props": {
              "text": "Address/URL"
            },
            "type": "ia.display.label"
          },
          {
            "events": {
              "dom": {
                "onKeyUp": {
                  "config": {
                    "script": "\tif event.key \u003d\u003d \u0027Enter\u0027:\n\t\tself.view.getChild(\"root\").submit()"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "LinkURL"
            },
            "position": {
              "basis": "32px"
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "bidirectional": true,
                    "path": "view.custom.url"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "placeholder": "https://..."
            },
            "type": "ia.input.text-field"
          }
        ],
        "meta": {
          "name": "Row 2"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "direction": "column",
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "draggable": true,
                    "id": "Hyperlink",
                    "modal": false,
                    "overlayDismiss": false,
                    "resizable": true,
                    "showCloseIcon": true,
                    "type": "close",
                    "viewPath": "",
                    "viewportBound": false
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Cancel"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/cancel"
                }
              },
              "primary": false,
              "style": {
                "paddingLeft": 4,
                "paddingRight": 4
              },
              "text": "Cancel"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "script": "\tself.view.getChild(\"root\").submit()"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Insert"
            },
            "position": {
              "shrink": 0
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/check_circle_outline"
                }
              },
              "style": {
                "paddingLeft": 4,
                "paddingRight": 4
              },
              "text": "Insert"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "Buttons"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "justify": "flex-end",
          "style": {
            "gap": 8,
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "justify": "space-between",
      "style": {
        "padding": "0.6em"
      }
    },
    "scripts": {
      "customMethods": [
        {
          "name": "submit",
          "params": [],
          "script": "\tif self.view.custom.text.strip() \u003d\u003d \u0027\u0027:\n\t\ttext \u003d self.view.custom.url\n\telse:\n\t\ttext \u003d self.view.custom.text\n\tsystem.perspective.sendMessage(\u0027insert-hyperlink\u0027, {\n\t\t\u0027domID\u0027: self.view.params.domID,\n\t\t\u0027text\u0027: text,\n\t\t\u0027url\u0027: self.view.custom.url\n\t})\n\tsystem.perspective.closePopup(\u0027Hyperlink\u0027)"
        }
      ],
      "extensionFunctions": null,
      "messageHandlers": []
    },
    "type": "ia.container.flex"
  }
}