{
  "custom": {
    "searching": false,
    "selected": false
  },
  "params": {
    "category": null,
    "description": "CRYING FACE",
    "domID": "",
    "number": 128546,
    "selected": false,
    "size": "200%"
  },
  "propConfig": {
    "custom.searching": {
      "access": "PRIVATE",
      "persistent": true
    },
    "custom.selected": {
      "access": "PRIVATE",
      "binding": {
        "config": {
          "path": "view.params.selected"
        },
        "type": "property"
      },
      "persistent": true
    },
    "params.category": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.description": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.domID": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.number": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selected": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.size": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 40,
      "width": 40
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Markdown"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.source": {
            "binding": {
              "config": {
                "expression": "\u0027\u003cdiv style\u003d\"position:relative; top: -2px;\"\u003e\u0026#\u0027 + {view.params.number} + \u0027;\u003c/div\u003e\u0027"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "markdown": {
            "escapeHtml": false
          },
          "sectionSpacing": 0,
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.display.markdown"
      }
    ],
    "events": {
      "dom": {
        "onClick": {
          "config": {
            "script": "\tif self.view.params.category is not None:\n\t\tsystem.perspective.sendMessage(\u0027set-category\u0027, {\n\t\t\t\u0027category\u0027: self.view.params.category\n\t\t})\n\telse:\n\t\tsystem.perspective.closePopup(\u0027Emojis\u0027)\n\t\tsystem.perspective.sendMessage(\u0027insert-emoji\u0027, {\n\t\t\t\u0027number\u0027: self.view.params.number,\n\t\t\t\u0027description\u0027: self.view.params.description,\n\t\t\t\u0027domID\u0027: self.view.params.domID\n\t\t})"
          },
          "scope": "G",
          "type": "script"
        }
      }
    },
    "meta": {
      "name": "root",
      "tooltip": {
        "enabled": true
      }
    },
    "propConfig": {
      "meta.tooltip.location": {
        "binding": {
          "config": {
            "expression": "if({view.params.category} !\u003d null,\r\n\t\u0027center-left\u0027,\r\n\t\u0027top\u0027\r\n)"
          },
          "type": "expr"
        }
      },
      "meta.tooltip.text": {
        "binding": {
          "config": {
            "expression": "if({view.params.category} !\u003d null,\r\n\t{view.params.category},\r\n\t{view.params.description}\r\n)"
          },
          "type": "expr"
        }
      },
      "props.style.backgroundColor": {
        "binding": {
          "config": {
            "expression": "if({view.custom.selected} \u0026\u0026 {view.params.size} \u003d \u0027120%\u0027 \u0026\u0026 !{view.custom.searching},\r\n\t\u0027#FFFFFF\u0027,\r\n\t\u0027\u0027\r\n)"
          },
          "type": "expr"
        }
      },
      "props.style.borderLeftColor": {
        "binding": {
          "config": {
            "expression": "if({view.custom.selected} \u0026\u0026 !{view.custom.searching},\r\n\t\u0027#F0F0F0\u0027,\r\n\t\u0027#00000000\u0027\r\n)"
          },
          "type": "expr"
        },
        "persistent": true
      },
      "props.style.borderRightColor": {
        "binding": {
          "config": {
            "expression": "if({view.custom.selected} \u0026\u0026 !{view.custom.searching},\r\n\t\u0027#F0F0F0\u0027,\r\n\t\u0027#00000000\u0027\r\n)"
          },
          "type": "expr"
        },
        "persistent": true
      },
      "props.style.borderTopColor": {
        "binding": {
          "config": {
            "expression": "if({view.custom.selected} \u0026\u0026 !{view.custom.searching},\r\n\t\u0027#F0F0F0\u0027,\r\n\t\u0027#00000000\u0027\r\n)"
          },
          "type": "expr"
        },
        "persistent": true
      },
      "props.style.fontSize": {
        "binding": {
          "config": {
            "path": "view.params.size"
          },
          "type": "property"
        }
      }
    },
    "props": {
      "alignItems": "center",
      "direction": "column",
      "justify": "center",
      "style": {
        "borderLeftColor": "#00000000",
        "borderLeftStyle": "solid",
        "borderLeftWidth": 1,
        "borderRightColor": "#00000000",
        "borderRightStyle": "solid",
        "borderRightWidth": 1,
        "borderTopColor": "#00000000",
        "borderTopStyle": "solid",
        "borderTopWidth": 1,
        "cursor": "pointer",
        "overflow": "visible"
      }
    },
    "scripts": {
      "customMethods": [],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "set-category",
          "pageScope": true,
          "script": "\tself.view.custom.selected \u003d payload[\u0027category\u0027] \u003d\u003d self.view.params.category",
          "sessionScope": false,
          "viewScope": false
        },
        {
          "messageType": "set-searching",
          "pageScope": true,
          "script": "\tself.view.custom.searching \u003d payload[\u0027searching\u0027]",
          "sessionScope": false,
          "viewScope": false
        }
      ]
    },
    "type": "ia.container.flex"
  }
}