{
  "custom": {
    "imgClick": "",
    "imgIDs": []
  },
  "params": {
    "attachmentsTable": "mes.markdown_attachments",
    "display": true,
    "enableHTML": true,
    "enabled": true,
    "html": "",
    "markdownText": "",
    "openLinksInNewTab": true
  },
  "propConfig": {
    "custom.imgClick": {
      "access": "PUBLIC",
      "onChange": {
        "enabled": null,
        "script": "\tsystem.perspective.openPopup(\u0027Images\u0027, \u0027Exchange/PanZoomImage/Carousel\u0027, {\n\t\t\t\u0027imgIDs\u0027: self.custom.imgIDs,\n\t\t\t\u0027imgClicked\u0027: str(currentValue.value),\n\t\t\t\u0027popupName\u0027: \u0027Images\u0027,\n\t\t\t\u0027attachmentsTable\u0027: self.params.attachmentsTable\n\t\t}, \u0027\u0027,\n\t\tposition \u003d {\n\t\t\t\u0027width\u0027: \u0027100%\u0027,\n\t\t\t\u0027height\u0027: \u0027100%\u0027\n\t\t},\n\t\tshowCloseIcon \u003d False,\n\t\tdraggable \u003d False)\n\tself.custom.imgClick \u003d \u0027\u0027"
      },
      "persistent": true
    },
    "custom.imgIDs": {
      "access": "PUBLIC",
      "persistent": true
    },
    "params.attachmentsTable": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.display": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.enableHTML": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.enabled": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.html": {
      "binding": {
        "config": {
          "expression": "if({view.params.display},\r\n\tif({view.params.enableHTML},\r\n\t\trunScript(\u0027exchange.markdownPlus.markdown.massage\u0027, 10, {view.params.markdownText}, {view.params.openLinksInNewTab}),\r\n\t\t{view.params.markdownText}\r\n\t),\r\n\t\u0027\u0027\r\n)"
        },
        "type": "expr"
      },
      "paramDirection": "output",
      "persistent": true
    },
    "params.markdownText": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.openLinksInNewTab": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {},
  "root": {
    "children": [
      {
        "meta": {
          "domId": "preview",
          "name": "Markdown"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "persistent": true
          },
          "props.markdown.escapeHtml": {
            "binding": {
              "config": {
                "expression": "!{view.params.enableHTML}"
              },
              "type": "expr"
            }
          },
          "props.markdown.skipHtml": {
            "binding": {
              "config": {
                "expression": "!{view.params.enableHTML}"
              },
              "type": "expr"
            }
          },
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.html"
              },
              "type": "property"
            }
          },
          "props.style.backgroundColor": {
            "binding": {
              "config": {
                "expression": "if({view.params.enabled},\r\n\t\u0027\u0027,\r\n\t\u0027#00000042\u0027\r\n)"
              },
              "type": "expr"
            }
          },
          "props.style.cursor": {
            "binding": {
              "config": {
                "expression": "if({view.params.enabled},\r\n\t\u0027\u0027,\r\n\t\u0027not-allowed\u0027\r\n)"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "markdown": {},
          "style": {
            "classes": "exchange/markdown-plus/markdown",
            "zIndex": 0
          }
        },
        "type": "ia.display.markdown"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}